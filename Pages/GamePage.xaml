<Page x:Class="Clicker.GamePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Clicker"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="GamePage">
    <Grid>
        <Button Name="btnFirstPage" Width="150" Height="75" Content="Back To The Home" VerticalAlignment="Top" HorizontalAlignment="Left" Click="btnFirstPage_Click"></Button>
        <Label Content="GAME PAGE" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="60"></Label>
    </Grid>
</Page>
